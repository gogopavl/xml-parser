    <body>

        Μια φορά κι έναν καιρό ζούσαν μερικά
        <segment id='1' features='errors;γραφ;γ-λεξ' state='active' subcorp='' filename=''>πολάκια</segment>
        που <segment id='2' features='errors;ορθ;ο-λεξ' state='active' subcorp='' filename=''>ανέβεναν</segment> στο δέντρο και
        <segment id='3' features='errors;γραφ;γ-γραμ' state='active' subcorp='' filename=''>κελαδιόσαν</segment>
        μέχρι που <segment id='4' features='errors;ορθ;ο-λεξ' state='active' subcorp='' filename=''>είρθε</segment>
        <segment id='39' features='errors;correct_συμφωνία;πτώση' state='active' subcorp='' filename=''>
            <segment id='11' features='errors;correct_συμφωνία;αριθμός' state='active' comment='σωστό' subcorp='' filename='' parent='39'>
                <segment id='9' features='errors;correct_συμφωνία;γένος' state='active' subcorp='' filename='' parent='11'>
                    <segment id='29' features='errors;correct_συμφωνία;πρόσωπο' state='active' subcorp='' filename='' parent='9'>μια</segment>
                </segment>
            </segment>
        </segment>
        γάτα και
        <segment id='27' features='errors;οψη;μσυν' state='active' subcorp='' filename=''>προσπαθούσε</segment>
        να <segment id='5' features='errors;χρ;πρλθ' state='active' subcorp='' filename=''>σκαρφαλοσε</segment> στο δέντρο και
        <segment id='6' features='errors;ορθ;ο-λεξ' state='active' subcorp='' filename=''>παραλήγο</segment>
        να τα φάει
        <segment id='42' features='errors;correct_συμφωνία;πτώση' state='active' subcorp='' filename=''>
            <segment id='30' features='errors;correct_συμφωνία;αριθμός' state='active' subcorp='' filename='' parent='42'>
                <segment id='40' features='errors;correct_συμφωνία;γένος' state='active' subcorp='' filename='' parent='30'>
                    <segment id='41' features='errors;correct_συμφωνία;πρόσωπο' state='active' subcorp='' filename='' parent='40'>τα</segment>
                </segment>
            </segment>
        </segment>
        μισά <segment id='8' features='errors;κειμ;συνδ;παρ-προσ' state='active' subcorp='' filename=''>και</segment> μέχρι που
        <segment id='7' features='errors;ορθ;ο-λεξ' state='active' subcorp='' filename=''>είρθε</segment>
        <segment id='43' features='errors;correct_συμφωνία;γένος' state='active' subcorp='' filename=''>
            <segment id='31' features='errors;correct_συμφωνία;αριθμός' state='active' comment='σωστό' subcorp='' filename='' parent='43'>
                <segment id='44' features='errors;correct_συμφωνία;πρόσωπο' state='active' subcorp='' filename='' parent='31'>
                    <segment id='45' features='errors;correct_συμφωνία;πτώση' state='active' subcorp='' filename='' parent='44'>ο</segment>
                </segment>
            </segment>
        </segment>
        <segment id='10' features='errors;ορθ;ο-λεξ' state='active' subcorp='' filename=''>σκιλος</segment> να <segment id='12'
                                                                                                                        features='errors;ορθ;ο-γραμ'
                                                                                                                        state='active' subcorp=''
                                                                                                                        filename=''>τραβίξη
    </segment>
        <segment id='32' features='errors;correct_συμφωνία;αριθμός' state='active' comment='σωστό' subcorp='' filename=''>
            <segment id='46' features='errors;correct_συμφωνία;γένος' state='active' subcorp='' filename='' parent='32'>
                <segment id='47' features='errors;correct_συμφωνία;πρόσωπο' state='active' subcorp='' filename='' parent='46'>
                    <segment id='48' features='errors;correct_συμφωνία;πτώση' state='active' subcorp='' filename='' parent='47'>την</segment>
                </segment>
            </segment>
        </segment>
        γάτα.<segment id='13' features='errors;ορθ;ο-λεξ' state='active' subcorp='' filename=''>Εφτηχός</segment> που
        δεν έφαγε
        <segment id='50' features='errors;correct_συμφωνία;πρόσωπο' state='active' subcorp='' filename=''>
            <segment id='49' features='errors;correct_συμφωνία;γένος' state='active' subcorp='' filename='' parent='50'>
                <segment id='14' features='errors;correct_συμφωνία;αριθμός' state='active' comment='σωστό' subcorp='' filename='' parent='49'>
                    <segment id='51' features='errors;correct_συμφωνία;πτώση' state='active' subcorp='' filename='' parent='14'>τα</segment>
                </segment>
            </segment>
        </segment>
        πουλάκια.Μετά <segment id='15' features='errors;ορθ;ο-λεξ' state='active' subcorp='' filename=''>είρθε</segment>
        <segment id='53' features='errors;correct_συμφωνία;πρόσωπο' state='active' subcorp='' filename=''>
            <segment id='35' features='errors;correct_συμφωνία;αριθμός' state='active' comment='σωστό' subcorp='' filename='' parent='53'>
                <segment id='52' features='errors;correct_συμφωνία;γένος' state='active' subcorp='' filename='' parent='35'>
                    <segment id='54' features='errors;correct_συμφωνία;πτώση' state='active' subcorp='' filename='' parent='52'>η</segment>
                </segment>
            </segment>
        </segment>
        <segment id='16' features='errors;τον;τ-παρ' state='active' subcorp='' filename=''>μαμα</segment>
        <segment id='36' features='errors;ορθ;ο-γραμ' state='active' subcorp='' filename=''>τον</segment>
        <segment id='17' features='errors;γραφ;γ-λεξ' state='active' subcorp='' filename=''>πολιόν</segment> και τρόμαξε από <segment id='20'
                                                                                                                                      features='errors;τον'
                                                                                                                                      state='active'
                                                                                                                                      subcorp=''
                                                                                                                                      filename=''>το
    </segment>
        <segment id='19' features='errors;ορθ;ο-λεξ' state='active' subcorp='' filename=''>σκίλο</segment> και
        <segment id='33' features='errors;correct_συμφωνία;αριθμός' state='active' comment='σωστό' subcorp='' filename=''>
            <segment id='18' features='errors;correct_συμφωνία;γένος' state='active' subcorp='' filename='' parent='33'>
                <segment id='37' features='errors;correct_συμφωνία;πρόσωπο' state='active' subcorp='' filename='' parent='18'>
                    <segment id='55' features='errors;correct_συμφωνία;πτώση' state='active' subcorp='' filename='' parent='37'>η</segment>
                </segment>
            </segment>
        </segment>
        <segment id='38' features='errors' state='active' subcorp='' filename=''>γάτα το έβαλε στα πόδια και
            <segment id='56' features='errors;correct_συμφωνία;γένος' state='active' subcorp='' filename='' parent='38'>
                <segment id='34' features='errors;correct_συμφωνία;αριθμός' state='active' comment='σωστό' subcorp='' filename='' parent='56'>
                    <segment id='57' features='errors;correct_συμφωνία;πρόσωπο' state='active' subcorp='' filename='' parent='34'>
                        <segment id='58' features='errors;correct_συμφωνία;πτώση' state='active' subcorp='' filename='' parent='57'>ο</segment>
                    </segment>
                </segment>
            </segment>
            <segment id='21' features='errors;ορθ;ο-λεξ' state='active' subcorp='' filename='' parent='38'>σκίλος</segment>
            έτρεξε να <segment id='22' features='errors;ορθ;ο-γραμ' state='active' subcorp='' filename='' parent='38'>πιάσι</segment>
            <segment id='28' features='errors;κειμ;αναφορά_κλιτικό;αντ_οφ' state='active' subcorp='' filename='' parent='38'>τιν γάτα</segment> και
            ζήσαν
            <segment id='24' features='errors;ορθ;ο-γραμ' state='active' subcorp='' filename='' parent='38'>εκείνι</segment>
            καλά και <segment id='25' features='errors;ορθ;ο-γραμ' state='active' subcorp='' filename='' parent='38'>εμίς</segment>
            <segment id='26' features='errors;ορθ;ο-λεξ' state='active' subcorp='' filename='' parent='38'>καλίτερα</segment>.
        </segment>
    </body>
